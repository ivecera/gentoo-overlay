index 1996f9d..22c81c6 100644
--- a/vmblock-only/shared/vm_assert.h
+++ b/vmblock-only/shared/vm_assert.h
@@ -251,12 +251,12 @@ void WarningThrottled(uint32 *count, const char *fmt, ...)
 #define LOG_ONCE(_s) DO_ONCE(Log _s)
 
 #ifdef VMX86_DEVEL
-   #define DEPRECATED(_fix) DO_ONCE(                                     \
+   #define VMWARE_DEPRECATED(_fix) DO_ONCE(                                     \
                                Warning("%s:%d: %s is DEPRECATED. %s\n",  \
                                        __FILE__, __LINE__, __FUNCTION__, \
                                        _fix))
 #else
-   #define DEPRECATED(_fix) do {} while (0)
+   #define VMWARE_DEPRECATED(_fix) do {} while (0)
 #endif
 
 
